package com.xq.conference.model;

import java.io.Serializable;

/**
 * 基础物品订单
 * @ClassName Basegoodsorder
 * @Author yangweihang
 * @Date 2018年9月5日 上午11:52:06
 */
public class Basegoodsorder implements Serializable{
	private Integer boid;//基础物品订单id
	private Integer ocid;//订单内容id
	private String boname;//物品名称
	private Integer bonum;//物品数量
	public Basegoodsorder() {
	}
	public Basegoodsorder(Integer boid, Integer ocid, String boname, Integer bonum) {
		this.boid = boid;
		this.ocid = ocid;
		this.boname = boname;
		this.bonum = bonum;
	}
	public Integer getBoid() {
		return boid;
	}
	public void setBoid(Integer boid) {
		this.boid = boid;
	}
	public Integer getOcid() {
		return ocid;
	}
	public void setOcid(Integer ocid) {
		this.ocid = ocid;
	}
	public String getBoname() {
		return boname;
	}
	public void setBoname(String boname) {
		this.boname = boname;
	}
	public Integer getBonum() {
		return bonum;
	}
	public void setBonum(Integer bonum) {
		this.bonum = bonum;
	}
	@Override
	public String toString() {
		return "Basegoodsorder [boid=" + boid + ", ocid=" + ocid + ", boname=" + boname + ", bonum=" + bonum + "]";
	}
}
