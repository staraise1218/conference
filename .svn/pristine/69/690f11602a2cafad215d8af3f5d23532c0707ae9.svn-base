package com.xq.conference.model;

/**
 * 人员权限
 * @ClassName UserPower
 * @Author yangweihang
 * @Date 2018年9月5日 下午2:59:43
 */
public class UserPower {
	private Integer upid;//人员权限id
	private Integer id;//人员id
	private Integer pid;//权限id
	private String upurl;//跳转路径
	private Integer ustatus;//1.停用 2.启用
	public UserPower() {
	}
	public UserPower(Integer upid, Integer id, Integer pid, String upurl, Integer ustatus) {
		this.upid = upid;
		this.id = id;
		this.pid = pid;
		this.upurl = upurl;
		this.ustatus = ustatus;
	}
	public Integer getUpid() {
		return upid;
	}
	public void setUpid(Integer upid) {
		this.upid = upid;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getPid() {
		return pid;
	}
	public void setPid(Integer pid) {
		this.pid = pid;
	}
	public String getUpurl() {
		return upurl;
	}
	public void setUpurl(String upurl) {
		this.upurl = upurl;
	}
	public Integer getUstatus() {
		return ustatus;
	}
	public void setUstatus(Integer ustatus) {
		this.ustatus = ustatus;
	}
	@Override
	public String toString() {
		return "UserPower [upid=" + upid + ", id=" + id + ", pid=" + pid + ", upurl=" + upurl + ", ustatus=" + ustatus
				+ "]";
	}
}
