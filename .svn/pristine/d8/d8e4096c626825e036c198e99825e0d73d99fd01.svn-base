package com.xq.conference.model;

/**
 * 会议室
 * @ClassName Room
 * @Author yangweihang
 * @Date 2018年9月5日 上午11:17:33
 */
public class Room {
	private Integer rid;//会议室id
	private String rname;//会议室名称 ----------------888
	private String rdepartment;//所属部门------获取
	private Integer rbudingnubmer;//楼号
	private Integer rfloor;//楼层
	private Integer rhousenumber;//门牌号
	private Integer rinterval;//间隔时间
	private String rlocation;//位置--------获取
	private String rintroduce;//简介
	private String rpic;//图片
	private String hcode;//医院编码-------获取
	private Integer rstate;//会议室状态(1.未锁定 2.锁定 3.已删除)
	private Integer rusestate;//会议室使用状态 (1.使用中  2.已预订  3.未预定)
	public Integer getRid() {
		return rid;
	}
	public void setRid(Integer rid) {
		this.rid = rid;
	}
	public String getRname() {
		return rname;
	}
	public void setRname(String rname) {
		this.rname = rname;
	}
	public String getRdepartment() {
		return rdepartment;
	}
	public void setRdepartment(String rdepartment) {
		this.rdepartment = rdepartment;
	}
	public Integer getRbudingnubmer() {
		return rbudingnubmer;
	}
	public void setRbudingnubmer(Integer rbudingnubmer) {
		this.rbudingnubmer = rbudingnubmer;
	}
	public Integer getRfloor() {
		return rfloor;
	}
	public void setRfloor(Integer rfloor) {
		this.rfloor = rfloor;
	}
	public Integer getRhousenumber() {
		return rhousenumber;
	}
	public void setRhousenumber(Integer rhousenumber) {
		this.rhousenumber = rhousenumber;
	}
	public Integer getRinterval() {
		return rinterval;
	}
	public void setRinterval(Integer rinterval) {
		this.rinterval = rinterval;
	}
	public String getRlocation() {
		return rlocation;
	}
	public void setRlocation(String rlocation) {
		this.rlocation = rlocation;
	}
	public String getRintroduce() {
		return rintroduce;
	}
	public void setRintroduce(String rintroduce) {
		this.rintroduce = rintroduce;
	}
	public String getRpic() {
		return rpic;
	}
	public void setRpic(String rpic) {
		this.rpic = rpic;
	}
	public String getHcode() {
		return hcode;
	}
	public void setHcode(String hcode) {
		this.hcode = hcode;
	}
	public Integer getRstate() {
		return rstate;
	}
	public void setRstate(Integer rstate) {
		this.rstate = rstate;
	}
	public Integer getRusestate() {
		return rusestate;
	}
	public void setRusestate(Integer rusestate) {
		this.rusestate = rusestate;
	}
	public Room(Integer rid, String rname, String rdepartment,
			Integer rbudingnubmer, Integer rfloor, Integer rhousenumber,
			Integer rinterval, String rlocation, String rintroduce,
			String rpic, String hcode, Integer rstate, Integer rusestate) {
		super();
		this.rid = rid;
		this.rname = rname;
		this.rdepartment = rdepartment;
		this.rbudingnubmer = rbudingnubmer;
		this.rfloor = rfloor;
		this.rhousenumber = rhousenumber;
		this.rinterval = rinterval;
		this.rlocation = rlocation;
		this.rintroduce = rintroduce;
		this.rpic = rpic;
		this.hcode = hcode;
		this.rstate = rstate;
		this.rusestate = rusestate;
	}
	public Room() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "Room [rid=" + rid + ", rname=" + rname + ", rdepartment="
				+ rdepartment + ", rbudingnubmer=" + rbudingnubmer
				+ ", rfloor=" + rfloor + ", rhousenumber=" + rhousenumber
				+ ", rinterval=" + rinterval + ", rlocation=" + rlocation
				+ ", rintroduce=" + rintroduce + ", rpic=" + rpic + ", hcode="
				+ hcode + ", rstate=" + rstate + ", rusestate=" + rusestate
				+ "]";
	}
	
}
